page {
    background-color: #f5f5f5;
}

.white-box {
    width: 720rpx;
    margin: 30rpx auto;
    background-color: #fff;
    border-radius: 12rpx;
    overflow: hidden;
    box-shadow: 0px 20rpx 20rpx -7rpx rgba(165, 181, 196, 0.1);
}

.box-title {
    width: fit-content;
    margin-left: 30rpx;
    padding: 20rpx 0;
    font-size: 28rpx;
    color: #8E98A3;
}

.white-box image {
    width: 50rpx;
    height: 50rpx;
}