.upload-resume {
    margin-top: 51rpx;
}

.upload-button,
.submit-button {
    width: 313rpx;
    height: 100rpx;
    background: rgba(246, 248, 250, 1);
    border-radius: 6rpx;
    border: 1rpx solid rgba(166, 191, 216, 1);
    text-align: center;
    line-height: 100rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(54, 59, 62, 1);
}

.upload-button {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.upload-button image {
    margin: 38rpx 18rpx 38rpx 81rpx;
    width: 20rpx;
    height: 24rpx;
}

.submit-button {
    width: 207rpx;
}

.footer-button {
    width: 480rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 0rpx auto;
}

.cancle,
.toudi {
    width: 210rpx;
    height: 80rpx;
    background: rgba(137, 137, 137, 1);
    border-radius: 12rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

.toudi {
    background: #C32E47;
}


/* 简历投递状态 弹窗 */
.status-pup-up {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 600rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 12rpx;
    z-index: 99999;
    padding: 50rpx 0rpx;
}

.status-title {
    width: fit-content;
    margin: 0rpx auto 30rpx auto;
    font-size: 36rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(54, 59, 62, 1);
}

.status-content {
    width: 90%;
    margin: 33rpx auto 27rpx auto;
    font-size: 26rpx;
}

.status-content-left {
    color: #898989;
}

.status-content-right {
    color: #363B3E;
}

.user-resume {
    margin-top: 25rpx;
    width: 540rpx;
    height: 90rpx;
    background: rgba(246, 248, 250, 1);
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(54, 59, 62, 1);
}

.user-resume image {
    margin: 25rpx 18rpx 28rpx 36rpx;
    width: 32rpx;
    height: 37rpx;
}

.status-line {
    margin: 36rpx auto 0rpx auto;
    width: fit-content;
    height: 5rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    overflow: visible;
}

.line {
    width: 139rpx;
    border: 1px solid #D8D8D8;

}

.status-line .pass,
.nopass,
.wait,
.wait-nopass {
    margin-top: -26rpx;
    width: 50rpx;
    height: 50rpx;
    border-radius: 100%;
    text-align: center;
    line-height: 50rpx;
}

.pass,
.nopass {
    border: 5rpx solid #FDBEB5;
    background-color: #FC7D6B;
}

.wait {
    border: 5rpx solid #D8D8D8;
    background-color: #fff;
}

.wait-nopass {
    border: 5rpx solid #D8D8D8;
    background-color: #fff;
}


.pass image {
    width: 28rpx;
    height: 20rpx;
}

.wait image {
    margin-bottom: 8rpx;
    width: 24rpx;
    height: 6rpx;
    margin-right: 3rpx;

}

.wait-nopass image {
    width: 20rpx;
    height: 20rpx;
}

.status-text {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 45rpx auto 0rpx auto;
    font-size: 20rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(54, 59, 62, 1);
    width: 469rpx;

}

.status-text-box {
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 20rpx;
    color: #363B3E;
}

.status-text-box view {
    margin-right: 5rpx;
    width: 12rpx;
    height: 12rpx;
    border-radius: 100%;
    background-color: #FC7D6B;
}

.select-content {
    width: 300rpx;
    height: fit-content;
    max-height: 155px;
    min-height: 30px;
    position: absolute;
    top: 47px;
    z-index: 999;
    left: 0px;
    display: flex;
    flex-direction: column;
    overflow: scroll;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 4rpx 10rpx 0px rgba(0, 0, 0, 0.1);
    padding: 0 10rpx;
    border-radius: 12rpx
}

.country {
    width: 96%;
    height: fit-content;
    max-height: 205px;
}

.select-content view {
    margin-left: 10rpx;
}