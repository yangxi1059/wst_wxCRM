::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
    display: none;
}

page {
    background: #F8F9FA;
    width: 100%;
    height: 100%;
}

.index-page {
    margin: 0 auto;
    width: 100%;
    height: 100%;
}

.index-fixed{
    position: fixed;
    top: 0;
    left: 0;
}

.detail-top {
    width: 100%;
    height: 115rpx;
    border-bottom: 1rpx solid rgb(226, 226, 226);
    background-color: #fff;
    margin: 0 auto;
    display: flex;
    flex-direction: row;
    align-items: center;
    /* padding-left: 45rpx; */
}

.detail-top-box {
    margin: 0 auto;
    width: 690rpx;
    display: flex;
    flex-direction: row;
}

.company-logo {
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
    border: 1rpx solid rgba(230, 234, 241, 1);
    margin-right: 20rpx;
}

.company-info {
    display: flex;
    flex-direction: column;

}

.company-info-up {
    display: block;
    font-size: 28rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(54, 59, 62, 1);
}

.company-info-down {
    display: block;
    margin-top: 10rpx;
    font-size: 22rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(142, 152, 163, 1);
}

.detail-center {
    margin: 30rpx auto;
    padding: 30rpx 25rpx 42rpx;
    width: 640rpx;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 20rpx 20rpx -7rpx rgba(165, 181, 196, 0.1);
    border-radius: 12rpx;
}

.title {
    font-size: 28rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: bold;
    color: rgba(54, 59, 62, 1);
}

.lable {
    margin-top: 20rpx;
    padding-bottom: 30rpx;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    border-bottom: 1rpx solid rgb(0, 0, 0, .1);
}

.lable-item {
    position: relative;
    display: flex;
    margin-bottom: 10rpx;
    padding-right: 5px;
    min-width: 300rpx;
    border-radius: 3px;
    font-size: 22rpx;
    font-weight: 400;
    color: #000000;
    align-items: center;
}

.lable-item text:last-child,
.lable-three text:last-child {
    color: #363B3E;
}

.lable-three {
    position: relative;
    display: flex;
    margin-bottom: 10rpx;
    min-width: 320rpx;
    width: fit-content;
    border-radius: 3px;
    padding-right: 10rpx;
    font-size: 22rpx;
    color: #000000;
    align-items: center;
}

.lable-three text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #000000;
}

.content-title {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin: 30rpx 0rpx 18rpx 0rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: bold;
    color: rgba(54, 59, 62, 1);
}

.content-title image {
    margin-right: 12rpx;
    width: 30rpx;
    height: 28rpx;
}

.content {
    width: 100%;
    height: fit-content;
    font-size: 22rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #363B3E;
    line-height: 40rpx;
    white-space: pre-wrap;
    word-wrap: break-word;
}

.button-box {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    height: 130rpx;
    background: rgba(255, 255, 255, 1);
    border-top: 1rpx solid rgb(165, 181, 196, 0.5);
}

.button-box-bottom{
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    height: 130rpx;
    background: rgba(255, 255, 255, 1);
    border-top: 1rpx solid rgb(165, 181, 196, 0.5);
    z-index: 9999;
}

/* .button-left, */
.button-right {
    margin: 25rpx auto;
    width: 660rpx;
    height: 80rpx;
    border-radius: 6rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    align-self: center;
    text-align: center;
}

.button-right {
    background: linear-gradient(90deg, rgba(190, 219, 225, 1) 0%, rgba(148, 205, 218, 1) 100%);
}

.button-toudi {
    margin: 25rpx auto;
    width: 660rpx;
    height: 80rpx;
    background: linear-gradient(90deg, rgba(190, 219, 225, 1) 0%, rgba(148, 205, 218, 1) 100%);
    border-radius: 6rpx;
    text-align: center;
    font-size: 26rpx;
    line-height: 80rpx;
    color: rgba(255, 255, 255, 1);
}

.button-box image {
    margin: 0 20rpx 0rpx 260rpx;
}

.button-left image {
    width: 28rpx;
    height: 24rpx;
}


.button-right image {
    width: 25rpx;
    height: 24rpx;
}

.button-box text {
    width: fit-content;
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

.active {
    background: rgba(243, 243, 244, 1);
}

.active text {
    color: rgba(54, 59, 62, 1);
    font-size: 24rpx;
}

/* 遮罩层 */
.mask {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: #000;
    opacity: 0.5;
    z-index: 999;
}

/* 投递简历弹窗 */
.pop-up {
    position: absolute;
    width: 100%;
    overflow: hidden;
    height: 100%;
}

.pop-up-top {
    width: 100%;
    position: absolute;
    top: 0;
    z-index: 999;
    background-color: #fff;
}

.pop-up-title {
    width: 670rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 43rpx auto 26rpx auto;
    font-size: 34rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(54, 59, 62, 1);
}

.pop-up-title text {
    font-size: 34rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(54, 59, 62, 1);
}

.pop-up-title image {
    width: 42rpx;
    height: 42rpx;
}


.pop-up-center {
    height: 100%;
    width: 670rpx;
    position: absolute;
    left: 0;
    right: 0;
    margin: 0 auto;
    overflow-y: auto;
    top: 0;
    -webkit-overflow-scrolling: touch;
}

.popup-center {
    margin-top: 120rpx;
    margin-bottom: 170rpx;
}

.fill-box {
    width: 670rpx;
    margin-bottom: 30rpx;
}

.fill-title {
    margin-right: 30rpx;
    margin-bottom: 14rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #898989;
}

.fill-content {
    position: relative;
    width: 95%;
    height: 80rpx;
    border-radius: 12rpx;
    border: 1px solid rgba(142, 152, 163, 0.2);
    margin: 0 auto;
    padding: 0 10rpx;
    padding-left: 20rpx;
    line-height: 80rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(54, 59, 62, 1);

}

.fill-content image {
    width: 29rpx;
    height: 14rpx;
    position: absolute;
    right: 25rpx;
    top: 34rpx;
}

.picker-box {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.picker-box .fill-content {
    width: 295rpx;
    margin: 0rpx;
}


.checkbox .wx-checkbox-input {
    width: 30rpx;
    height: 30rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 2rpx;
    border: 1rpx solid rgba(166, 191, 216, 1);
}

.checkbox .wx-checkbox-input-disabled {
    width: 30rpx;
    height: 30rpx;
    background: rgba(255, 255, 255, 1);
    border-radius: 2rpx;
    border: 1rpx solid rgba(166, 191, 216, 1);
}

.checkbox text {
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(54, 59, 62, 1);
}

.fill-box-line,
.upload-resume {
    width: 100%;
    margin: 0 auto 34rpx auto;
    display: flex;
    flex-direction: row;
    align-items: center;
    align-self: center;
}

.fill-box-line .fill-title {
    margin-top: 15rpx;
}


.fill-checkbox {
    width: fit-content;
    display: flex;
    flex-direction: row;
    align-items: center;
    align-self: center;
}

.check-box:last-child {
    margin-left: 40rpx;
}

.city-box {
    margin-top: 20rpx;
}

.resume-box {
    width: 99%;
    height: 100rpx;
    border-radius: 12rpx;
    border: 1rpx solid rgba(142, 152, 163, 0.4);
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.resume-left-box {
    margin-left: 35rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.filename-box {
    display: flex;
    flex-direction: column;
}

.filename-box text:first-child {
    font-size: 26rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(54, 59, 62, 1);
}

.filename-box text:last-child {
    margin-top: 4rpx;
    display: block;
    font-size: 22rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #8E98A3;
}

.resume-right-box {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-right: 20rpx;
}

.resume-right-box image {
    margin-right: 12rpx;
    width: 28rpx;
    height: 20rpx;
}

.resume-right-box text {
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: rgba(142, 152, 163, 1);
}

.resume-box .checkbox {
    margin: 0rpx 20rpx;
}

.checkBox-box {
    margin: 0 18rpx 0rpx 23rpx;
    width: 20rpx;
    height: 20rpx;
    border: 2rpx solid #A9ABAD;
    border-radius: 100%;
    line-height: 20rpx;
}

.yuan {
    margin: 4rpx auto;
    width: 13rpx;
    height: 13rpx;
    background: #A9ABAD;
    border-radius: 100%;
}

.selectBox {
    border: 1rpx solid #C32E47;
    transition-duration: .2s;
}

.select {
    border: 2rpx solid rgba(195, 46, 71, 1);
    transition-duration: .2s;
}

.selected {
    background: rgba(195, 46, 71, 1);
    transition-duration: .2s;
}

.resume-left-box image {
    margin-right: 18rpx;
    width: 48rpx;
    height: 56rpx;
}

